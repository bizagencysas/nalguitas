{
"compilerOptions": {
  "target": "ES2022",
  "module": "CommonJS",
  "moduleResolution": "node",
  "outDir": "./dist",
  "rootDir": "./",
  "strict": false,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "resolveJsonModule": true,
  "lib": ["ES2022"]
},
"include": ["backend/**/*"],
"exclude": ["node_modules", "dist"]
}
