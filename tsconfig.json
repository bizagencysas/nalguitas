{
"compilerOptions": {
  "target": "ES2020",
  "module": "commonjs",
  "lib": ["ES2020"],
  "outDir": "./dist",
  "rootDir": "./",
  "strict": false,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "resolveJsonModule": true,
  "moduleResolution": "node"
},
"include": ["backend/**/*"],
"exclude": ["node_modules", "dist"]
}
